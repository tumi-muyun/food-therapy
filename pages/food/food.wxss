/**index.wxss**/
@import '../../templates/productList/productList.wxss';
page {
  background: #f5f5f5;
}
.header {
  display: flex;
  background: #fff;
  padding: 10rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
}
.location {
  width: 100rpx;
  text-align: center;
}
.inp {
  flex: 1;
  background: #f5f5f5;
  border-radius: 16rpx;
  padding-left: 10rpx;
  color: #666;
}
.nav {
  background: #fff;
  padding: 10rpx;
  display: flex;
  flex-wrap: wrap;
  text-align: center;
  font-size: 26rpx;
  color: #666;
}
.item {
  width: 25%;
  padding: 10rpx 0;
}
.img image {
  width: 74rpx;
  height: 74rpx;
}